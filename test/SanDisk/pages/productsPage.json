{
    "Sort By Menu": {
        "type": "css",
        "selector": "#dr_ordering",
        "items": "option[value]"
    },
    "Sort By": {
        "type": "css",
        "selector": "label.dr_catSortOptions"
    },
    "Category Title": {
        "type": "css",
        "selector": ".dr_categoryName"
    },
    "Products Price": {
        "type": "css",
        "selector": ".dr_price"
    },
    "Product Item Name": {
        "isCollection": true,
        "type": "css",
        "selector": "#dr_ordering"
    },
    "Capacity List": {
        "type": "css",
        "selector": "#dr_narrowResults",
        "items": "li.dr_facetValue a"
    },
    "Icon Basket": {
        "type": "css",
        "selector": "i.dr_icon.icon-basket"
    },
    "Basket": {
        "type": "css",
        "selector": ".dr_icon.icon-basket"
    },
    "Checkout Button": {
        "type": "css",
        "selector": "a.btn_checkOut"
    },
    "List of products": {
        "selector": ".col-md-9",
        "items": "a.dr_productName"
    },
    "Selected Filters Title": {
        "type": "css",
        "selector": "#dr_selectedFacets"
    },
    "Basket Icon": {
        "type": "css",
        "selector": "i[class$='basket']"
    },
    "Number Of Products Icon": {
        "type": "css",
        "selector": "#dr_cartQty"
    },
    "Mini Cart Text": {
        "type": "css",
        "selector": "#dr_miniCartDisplay span"
    },
    "List Of Usb Products": {
        "isCollection": true,
        "type": "css",
        "selector": "a.dr_productName"
    },
    "|List Of Products":{
        "type": "css",
        "selector": "#dr_products .row .row",
        "children" : {
            "Title": {
                "type": "css",
                "selector": ".prod-list-head"
            }
        }
    }
}